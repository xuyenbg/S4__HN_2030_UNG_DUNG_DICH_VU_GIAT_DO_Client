<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="editLogin" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxCornerRadiusTopStart">6dp</item>
        <item name="boxCornerRadiusTopEnd">6dp</item>
        <item name="boxCornerRadiusBottomStart">6dp</item>
        <item name="boxCornerRadiusBottomEnd">6dp</item>
        <item name="boxStrokeColor">#AAAAAA</item>
        <item name="boxBackgroundColor">@color/white</item>
        <item name="startIconTint">@color/black</item>
        <item name="passwordToggleTint">@color/black</item>
    </style>
    <style name="text_font_inter_light">
        <item name="fontFamily">@font/inter_light</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="text_font_inter_light_italic">
        <item name="fontFamily">@font/inter_light</item>
        <item name="android:textStyle">italic</item>
    </style>
    <style name="text_font_inter_regular">
        <item name="fontFamily">@font/inter_regular</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="text_font_inter_medium">
        <item name="fontFamily">@font/inter_medium</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="text_font_inter_semi_bold">
        <item name="fontFamily">@font/inter_semi_bold</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="text_font_inter_bold">
        <item name="fontFamily">@font/inter_bold</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="font_14_black_bold" parent="text_font_inter_bold">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="font_16_black_bold" parent="font_14_black_bold">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="font_14_black_medium" parent="text_font_inter_medium">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="font_14_primary_medium" parent="text_font_inter_medium">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/colorPriamry</item>
    </style>

    <style name="font_14_dask_blue_medium" parent="text_font_inter_medium">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/dark_blue</item>
    </style>

    <style name="font_12_white_medium" parent="text_font_inter_medium">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="font_12_gray_medium" parent="text_font_inter_medium">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/gray</item>
    </style>

    <style name="font_14_gray_medium" parent="text_font_inter_medium">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/gray</item>
    </style>

    <style name="font_12_black_medium" parent="text_font_inter_medium">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="font_20_white_bold" parent="text_font_inter_bold">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    
    <style name="CheckBox" parent="Widget.AppCompat.CompoundButton.CheckBox">
        <item name="android:button">@null</item>
        <item name="colorControlActivated">#828282</item>
        <item name="android:buttonTint">@null</item>
        <item name="fontFamily">@font/inter_regular</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="item_bottom_avigation_custom">
        <item name="android:textColor">@color/gray</item>
        <item name="fontFamily">@font/inter_semi_bold</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="item_bottom_avigation_custom_selected">
        <item name="android:textColor">@color/colorPriamry</item>
        <item name="fontFamily">@font/inter_semi_bold</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
    </style>

</resources>